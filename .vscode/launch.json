{
   "version": "0.2.0",
   "configurations": [{
        "name": "Python: Streamlit",
        "type": "python",
        "request": "launch",
        "module": "streamlit",
        "env": {
            "STREAMLIT_APP": "streamlit_app.py",
            "STREAMLIT_ENV": "development",
            "PYTHONPATH": "/bin",
        },
        "args": [
            "run",
            "./streamlit_app.py"
        ],
        "jinja": true
    }
   ]
}